{
  "name": "vue-imagewall",
  "version": "0.0.0-development",
  "description": "An interactive vue component that displays multiple images in a row",
  "homepage": "https://github.com/asvrada/vue-imagewall",
  "private": false,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build --target lib --name VueImageWall ./src/install.js",
    "lint": "vue-cli-service lint",
    "semantic-release": "semantic-release"
  },
  "main": "dist/VueImageWall.umd.js",
  "files": [
    "dist/*",
    "src/*"
  ],
  "dependencies": {
    "lodash": "^4.17.15",
    "vue": "^2.6.10"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.11.0",
    "@vue/cli-plugin-eslint": "^3.11.0",
    "@vue/cli-service": "^3.11.0",
    "node-sass": "^4.12.0",
    "rollup": "^1.24.0",
    "rollup-plugin-buble": "^0.19.8",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-vue": "^5.0.1",
    "sass-loader": "^8.0.0",
    "semantic-release": "^17.2.1",
    "vue-template-compiler": "^2.6.10"
  },
  "keywords": [
    "front-end",
    "vue",
    "image",
    "imagewall",
    "interactive"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/asvrada/vue-imagewall/issues"
  },
  "babel": {
    "presets": [
      "@vue/app"
    ]
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/asvrada/imagewall.git"
  }
}
