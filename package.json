{
  "name": "vue-image-wall",
  "version": "0.0.0-development",
  "description": "An interactive vue component that displays multiple images in a row",
  "homepage": "https://github.com/asvrada/vue-image-wall",
  "private": false,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "rollup -c",
    "lint": "vue-cli-service lint",
    "semantic-release": "semantic-release"
  },
  "main": "dist/vue-image-wall.umd.js",
  "module": "dist/vue-image-wall.esm.js",
  "files": [
    "dist/*",
    "src/*"
  ],
  "dependencies": {
    "lodash": "^4.17.15",
    "vue": "^2.6.10"
  },
  "peerDependencies": {
    "lodash": "^4.17.15",
    "vue": "^2.6.10"
  },
  "devDependencies": {
    "@rollup/plugin-buble": "^0.21.3",
    "@rollup/plugin-commonjs": "^16.0.0",
    "@rollup/plugin-node-resolve": "^10.0.0",
    "@vue/cli-plugin-babel": "^4.5.8",
    "@vue/cli-plugin-eslint": "^4.5.8",
    "@vue/cli-service": "^4.5.8",
    "node-sass": "^4.12.0",
    "rollup": "^2.32.1",
    "rollup-plugin-filesize": "^9.0.2",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-vue": "^5.0.1",
    "sass-loader": "^10.0.4",
    "semantic-release": "^17.2.1",
    "vue-template-compiler": "^2.6.10"
  },
  "keywords": [
    "front-end",
    "vue",
    "image",
    "imagewall",
    "interactive"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/asvrada/vue-image-wall/issues"
  },
  "babel": {
    "presets": [
      "@vue/app"
    ]
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/asvrada/vue-image-wall.git"
  }
}
